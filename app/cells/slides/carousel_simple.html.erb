<% if @attach.nil? || @attach.photos.blank? %>
    <i class="<%= if @attach.nil?
                    'fa fa-bolt'
                  else
                    'fa fa-spinner fa-spin'
                  end %> icon-large">
    </i>
<% else %>
    <% @photos.each do |item| %>
        <%= image_tag item.image, :class => "img-responsive img-thumbnail", :style => "width:100%;margin-left:12px;" %>
    <% end %>
<% end %>
